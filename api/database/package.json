{
  "name": "@tlauv/api-database",
  "scripts": {
    "prepush": "npm run test:depcheck; npm test",
    "start": "node index.js",
    "start:watch": "nodemon --watch '**.js' --watch '**.spec.js' --exec 'reset; sleep 3; npm start'",
    "test": "mocha index.spec.js",
    "test:watch": "nodemon --watch '**.js' --watch '**.spec.js' --exec 'reset; npm test'",
    "test:depcheck": "depcheck",
    "package": "rm -rf ${npm_package_name##*-}.zip; zip -r ${npm_package_name##*-}.zip ../../api/${npm_package_name##*-} > packageLog.log"
  },
  "dependencies": {
    "@hbkapps/convict": "^4.1.0",
    "@hbkapps/convict-provider-awsssm": "^0.1.2",
    "@hbkapps/logger": "^3.1.4",
    "crypto-extra": "^0.4.0",
    "express": "^4.16.4",
    "express-lambda-helper": "^0.1.1",
    "islambdawarm": "^1.1.0",
    "lambda-request-context": "^1.1.0",
    "micromustache": "^5.4.0",
    "mongodb": "^3.1.10"
  }
}
